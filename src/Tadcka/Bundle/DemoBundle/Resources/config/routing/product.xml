<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="tadcka_demo_product_index" path="/product">
        <default key="_controller">tadcka_demo.controller.product:indexAction</default>
    </route>

    <route id="tadcka_demo_product_old_index" path="/products">
        <default key="_controller">tadcka_demo.controller.product:oldIndexAction</default>
    </route>

</routes>
